{
  "version": "1.0.0-beta9",

  "buildOptions": {
    "preserveCompilationContext": true,
    "warningsAsErrors": true
  },

  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.1.0"
    },
    "dotnet-test-xunit": "2.2.0-*",
    "xunit": "2.1.0-*",
    "FluentBehave.Tools": "1.0.0-beta9"
  },

  "frameworks": {
    "netcoreapp1.1": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
      ]
    }
  },

  "testRunner": "xunit",

  "tooling": {
    "defaultNamespace": "FluentBehave.Tools.Tests"
  }
}
